{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "lib": [
        "esnext",
        "webworker"
      ]
    },
    "include": [
      "src/**/*.ts",
    ],
    "exclude": [
      "node_modules"
    ]
  }
  